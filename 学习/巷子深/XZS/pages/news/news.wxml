<XZSheader></XZSheader>
<view class="news">
  <view class="box" wx:for="{{newsArr}}" wx:key="_id">
    <xzs-news-item item="{{item}}">
    </xzs-news-item>
  </view>
  <view class="loading">
    <van-loading color="var(--themeColor)" wx:if="{{loading}}"/>
    <view class="notData" wx:else="{{isData}}">没有更多数据了~~</view>
  </view>

</view>